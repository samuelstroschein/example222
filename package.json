{
	"name": "@inlang/paraglide-js-adapter-sveltekit-example",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"_dev": "vite dev",
		"build": "paraglide-js compile --namespace sveltekit-example && vite build",
		"test": "paraglide-js compile --namespace sveltekit-example && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@inlang/paraglide-js": "*",
		"@inlang/plugin-message-format": "*",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module"
}
